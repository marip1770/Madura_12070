<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <!-- Sequences for sale.order -->
        <record id="seq_lembur" model="ir.sequence">
            <field name="name">lembur</field>
            <field name="code">lembur</field>
            <field name="prefix">OT</field>
            <field name="padding">9</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_lembur_biling" model="ir.sequence">
            <field name="name">lembur_biling</field>
            <field name="code">lembur_biling</field>
            <field name="prefix">OTBIL</field>
            <field name="padding">9</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_lembur_payroll" model="ir.sequence">
            <field name="name">lembur_payroll</field>
            <field name="code">lembur_payroll</field>
            <field name="prefix">OTP</field>
            <field name="padding">9</field>
            <field name="company_id" eval="False"/>
        </record>


    </data>
</odoo>