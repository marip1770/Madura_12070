            <field name="view_mode">tree</field>
            <field name="view_id" ref="bisa_hauling.view_tbl_fuel_truck_assigment_tree"/>
            <field name="act_window.id" ref="menu_action_fuel_truck_assigment"/>
        </record>

        <record id="view_tbl_fuel_truck_assigment_form_view" model="ir.actions.act_window.view">
