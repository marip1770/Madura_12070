<odoo>
    <data>
        <!-- Fuel Truck -->
        <!-- Fuel Truck Kontrak -->
        <record id="view_tbl_fuel_truck_kontrak_tree" model="ir.ui.view">
            <field name="name">tbl_bisa_fuel_truck_kontrak.tree</field>
            <field name="model">tbl_bisa_hauling_kontrak</field>
            <field name="priority" eval="50"/>
            <field name="arch" type="xml">
                <tree string="Contract" create="false">
                    <field name="nomor"/>
                    <field name="produk"/>
                    <field name="vendor"/>
                    <field name="lokasi"/>
                    <field name="nama_projek"/>
                    <field name="tgl_awal"/>
                    <field name="tgl_akhir"/>
                    <field name="nilai_kontrak" invisible="1"/>
                    <field name="state"/>
                    <field name="tipe" invisible="1"/>
                </tree>
            </field>
        </record>

        <record id="view_tbl_fuel_truck_kontrak_form" model="ir.ui.view">
            <field name="name">tbl_bisa_fuel_truck_kontrak.form</field>
            <field name="model">tbl_bisa_hauling_kontrak</field>
            <field name="priority" eval="50"/>
            <field name="arch" type="xml">
                <form string="Contract" create="false" edit="false">
                      <header>
                        <button string="Confirm" name="action_create_plan" type="object" class="oe_highlight"/>
                        <field name="state" widget="statusbar" statusbar_visible="draft,confirm"/>
                    </header>
                    <sheet>
                        <group>
                            <group>
                                <field name="nomor"/>
                                <field name="produk"/>
                                <field name="vendor"/>
                                <field name="lokasi"/>
                                <field name="nama_projek"/>
                                <field name="tgl_awal"/>
                                <field name="tgl_akhir"/>
                                <field name="quantity"/>
                                <field name="qty_day"/>
                                <field name="uom"/>
                                <field name="harga"/>
                                <field name="total"/>
                                <field name="nilai_kontrak" invisible="1"/>
                                <field name="tipe" invisible="1"/>
                            </group>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>
      
        <!-- Action Fuel Truck Kontrak -->
        <record model="ir.actions.act_window" id="menu_action_fuel_truck_kontrak">
            <field name="name">Contract</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">tbl_bisa_hauling_kontrak</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
            <field name="view_id" ref="view_tbl_fuel_truck_kontrak_tree"/>
            <field name="domain" >[('tipe','=','fuel_truck')]</field>
        </record>

        <record id="view_tbl_fuel_truck_kontrak_tree_view" model="ir.actions.act_window.view">
            <field eval="1" name="sequence"/>
            <field name="view_mode">tree</field>
            <field name="view_id" ref="bisa_hauling.view_tbl_fuel_truck_kontrak_tree"/>
            <field name="act_window.id" ref="menu_action_fuel_truck_kontrak"/>
        </record>

        <record id="view_tbl_fuel_truck_kontrak_form_view" model="ir.actions.act_window.view">
            <field eval="2" name="sequence"/>
            <field name="view_mode">form</field>
            <field name="view_id" ref="bisa_hauling.view_tbl_fuel_truck_kontrak_form"/>
            <field name="act_window.id" ref="menu_action_fuel_truck_kontrak"/>
        </record>

        <!-- Sub Menu Fuel Truck Kontrak-->
        <menuitem name="Contract" id="menu_fuel_truck_kontrak_left" parent="menu_fuel_truck_kontrak_top" action="menu_action_fuel_truck_kontrak" sequence="10" groups="bisa_hauling.grup_fuel_truck_usr"/>
        
        <!-- Action Fuel Truck Plan -->
        <record id="view_tbl_fuel_truck_plan_tree" model="ir.ui.view">
            <field name="name">tbl_bisa_fuel_truck_plan.tree</field>
            <field name="model">tbl_bisa_hauling_plan</field>
            <field name="priority" eval="50"/>
            <field name="arch" type="xml">
                <tree string="Planning">
                    <field name="tanggal"/>
                    <field name="no_kontrak"/>
                    <field name="produk"/>
                    <field name="vendor"/>
                    <field name="lokasi"/>
                    <field name="nama_projek"/>
                    <field name="quantity"/>
                    <field name="uom"/>
                    <field name="state"/>
                    <field name="total" invisible="1"/>
                    <field name="nilai" invisible="1"/>
                    <field name="tipe" invisible="1"/>
                    <!-- <button string="Assigment" name="open_wizard" type="object" class="oe_highlight"/> -->
                </tree>
            </field>
        </record>

        <record id="view_tbl_fuel_truck_plan_form" model="ir.ui.view">
            <field name="name">tbl_bisa_fuel_truck_plan.form</field>
            <field name="model">tbl_bisa_hauling_plan</field>
            <field name="priority" eval="50"/>
            <field name="arch" type="xml">
                <form string="Planning">
                    <header>
                        <button string="Confirm" name="action_create_assigment" type="object" class="oe_highlight" states="draft"/>
                        <field name="state" widget="statusbar" statusbar_visible="draft,confirm"/>
                    </header>
                    <sheet>
                        <group>
                            <group>
                                <field name="tanggal" readonly="1"/>
                                <field name="no_kontrak" readonly="1"/>
                                <field name="produk" readonly="1"/>
                                <field name="vendor" readonly="1"/>
                                <field name="lokasi" readonly="1"/>
                                <field name="nama_projek" readonly="1"/>
                                <field name="quantity" readonly="1"/>
                                <field name="uom" readonly="1"/>
                                <field name="total" invisible="1" readonly="1"/>
                                <field name="nilai" invisible="1" readonly="1"/>
                                <field name="tipe" invisible="1" readonly="1"/>
                            </group>
                        </group>
                    <notebook>
                            <page string="Detail">
                                <button string="Get" name="get_schedule_um" type="object" class="btn-primary"/>
                                <field name="detail_assigment_line">
                                    <tree editable="bottom" create="false">
                                        <field name="no_kontrak" invisible="1"/>
                                        <field name="driver"/>
                                        <field name="tipe_driver" invisible="1"/>
                                        <field name="unit_id" domain="[('tipe','=', tipe_driver)]"/>
                                        <field name="lokasi"/>
                                        <field name="route" domain="[('lokasi','=',lokasi)]"/>
                                        <field name="lokasi_asal"/>
                                        <field name="lokasi_tujuan"/>
                                        <!-- <field name="tipe" invisible="1"/> -->
                                    </tree>
                                </field>
                            </page>
                    </notebook>
                    </sheet>
                </form>
            </field>
        </record>

        <!-- Action Fuel Truck Plan -->
        <record model="ir.actions.act_window" id="menu_action_fuel_truck_plan">
            <field name="name">Planning</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">tbl_bisa_hauling_plan</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
            <field name="view_id" ref="view_tbl_fuel_truck_plan_tree"/>
            <field name="domain" >[('tipe','=','fuel_truck')]</field>
        </record>

        <record id="view_tbl_fuel_truck_plan_tree_view" model="ir.actions.act_window.view">
            <field eval="1" name="sequence"/>
            <field name="view_mode">tree</field>
            <field name="view_id" ref="bisa_hauling.view_tbl_fuel_truck_plan_tree"/>
            <field name="act_window.id" ref="menu_action_fuel_truck_plan"/>
        </record>
        
        <record id="view_tbl_fuel_truck_plan_form_view" model="ir.actions.act_window.view">
            <field eval="1" name="sequence"/>
            <field name="view_mode">form</field>
            <field name="view_id" ref="bisa_hauling.view_tbl_fuel_truck_plan_form"/>
            <field name="act_window.id" ref="menu_action_fuel_truck_plan"/>
        </record>

        <!-- Sub Menu Fuel Truck Plan -->
        <menuitem name="Planning" id="menu_fuel_truck_plan_left" parent="menu_fuel_truck_pna_top" action="menu_action_fuel_truck_plan" sequence="10" groups="bisa_hauling.grup_fuel_truck_usr"/>
        
        <!-- Fuel Truck Assigment -->
        <record id="view_tbl_fuel_truck_assigment_tree" model="ir.ui.view">
            <field name="name">tbl_bisa_fuel_truck_assigment.tree</field>
            <field name="model">tbl_bisa_hauling_assigment</field>
            <field name="priority" eval="50"/>
            <field name="arch" type="xml">
                <tree string="Assigment">
                    <field name="tanggal"/>
                    <field name="produk"/>
                    <field name="no_kontrak"/>
                    <field name="nama_projek"/>
                    <field name="name"/>
                    <field name="lokasi"/>
                    <field name="route"/>
                    <field name="lokasi_asal"/>
                    <field name="lokasi_tujuan"/>
                    <field name="driver"/>
                    <field name="user_driver" invisible="1"/>
                    <field name="unit_id"/>
                    <field name="qty_rencana"/>
                    <field name="qty_aktual"/>
                    <field name="uom"/>
                    <field name="state"/>
                    <field name="tipe" invisible="1"/>
                </tree>
            </field>
        </record>

        <record id="view_tbl_fuel_truck_assigment_form" model="ir.ui.view">
            <field name="name">tbl_bisa_fuel_truck_assigment.form</field>
            <field name="model">tbl_bisa_hauling_assigment</field>
            <field name="priority" eval="50"/>
            <field name="arch" type="xml">
                <form string="Assigment">
                    <header>
                        <button name="action_p5m" type="object" string="P5M" class="oe_highlight" states="start"/>
                        <button name="action_kesiapan" type="object" string="Kesiapan" class="oe_highlight" states="p5m"/>
                        <button name="action_p2h" type="object" string="P2H" class="oe_highlight" states="kesiapan"/>
                        <button name="action_operasional" type="object" string="Operasional" class="oe_highlight" states="p2h"/>
                        <button name="action_done" type="object" string="Done" class="oe_highlight" states="operasional"/>
                        <field name="state" widget="statusbar" statusbar_visible="start,p5m,kesiapan,p2h,operasional,done,maintenance,cancel"/>                    
                    </header>
                    <sheet>
                        <group>
                            <group>
                                <field name="tanggal" readonly="1"/>
                                <field name="name" readonly="1"/>
                                <field name="no_kontrak" readonly="1"/>
                                <field name="nama_projek" readonly="1"/>
                                <field name="lokasi" readonly="1"/>
                                <field name="produk" readonly="1"/>
                            </group>
                            <group>
                                <field name="driver" readonly="1"/>
                                <field name="user_driver" readonly="1" invisible="1"/>
                                <field name="unit_id" readonly="1"/>
                                <field name="route" readonly="1"/>
                                <field name="lokasi_asal" readonly="1" invisible="1"/>
                                <field name="lokasi_tujuan" readonly="1" invisible="1"/>
                                <field name="qty_rencana" readonly="1"/>
                                <field name="qty_aktual" readonly="1"/>
                                <field name="uom" readonly="1"/>
                                <field name='field_jumlah_nilai' readonly="1" invisible="1"/>
                                <field name="tipe" invisible="1" readonly="1"/>
                            </group>
                        </group>
                        <notebook>
                            <page string="Operasional" attrs="{'invisible': [('state', 'in', ('start', 'p5m', 'kesiapan', 'p2h'))]}">
                                <field name="operasional_line">
                                    <tree editable="bottom">
                                        <field name="tanggal" readonly="1"/>
                                        <field name="user_driver" invisible="1" readonly="1"/>
                                        <field name="driver" readonly="1"/>
                                        <field name="id_unit" readonly="1"/>
                                        <field name="lokasi" readonly="1" invisible="1"/>
                                        <field name="route" readonly="1"/>
                                        <field name="start_time"/>
                                        <field name="geo_start_longtitude"/>
                                        <field name="geo_start_latitude"/>
                                        <field name="end_shift"/>
                                        <field name="geo_shift_end_longtitude"/>
                                        <field name="geo_shift_end_latitude"/>
                                        <field name="start_shift"/>
                                        <field name="geo_shift_start_longtitude"/>
                                        <field name="geo_shift_start_latitude"/>
                                        <field name="end_time"/>
                                        <field name="geo_end_longtitude"/>
                                        <field name="geo_end_latitude"/>
                                        <field name="start_pengisian"/>
                                        <field name="geo_pengisian_start_longtitude"/>
                                        <field name="geo_pengisian_start_latitude"/>
                                        <field name="end_pengisian"/>
                                        <field name="geo_pengisian_end_longtitude"/>
                                        <field name="geo_pengisian_end_latitude"/>
                                        <field name="fuel_meter_start"/>
                                        <field name="fuel_meter_end"/>
                                        <field name="hm_km"/>
                                        <field name="picture"/>
                                        <field name="tipe" invisible="1"/>
                                    </tree>
                                </field>
                            </page>
                             <page string="P5M" attrs="{'invisible': [('state', 'in', ('start', 'kesiapan', 'p2h', 'operasional'))]}">
                                <field name="p5m_line">
                                    <tree editable="bottom" create="false">
                                        <field name="user_driver" invisible="1" readonly="1"/>
                                        <field name="field_pertanyaan_p5m" readonly="1"/>
                                        <field name="normal_value" invisible="1" readonly="1"/>
                                        <field name="field_jawaban_p5m"/>
                                        <field name="tipe" invisible="1"/>
                                    </tree>
                                </field>
                            </page>
                            <page string="Kesiapan" attrs="{'invisible': [('state', 'in', ('start', 'p5m', 'p2h', 'operasional'))]}">
                                <field name="kesiapan_line">
                                    <tree editable="bottom" create="false">
                                        <field name="user_driver" invisible="1" readonly="1"/>
                                        <field name="field_pertanyaan_kesiapan" readonly="1"/>
                                        <field name="normal_value" invisible="1" readonly="1" widget="float_time"/>
                                        <field name="field_jawaban_kesiapan" widget="float_time"/>
                                        <field name="tipe" invisible="1"/>
                                    </tree>
                                </field>
                            </page>
                            <page string="P2H" attrs="{'invisible': [('state', 'in', ('start', 'p5m', 'kesiapan', 'operasional'))]}">
                                <field name="p2h_line">
                                    <tree editable="bottom" create="false">
                                        <field name="unit_id" invisible="1" readonly="1"/>
                                        <field name="field_pertanyaan_p2h" readonly="1"/>
                                        <field name="normal_value" invisible="1" readonly="1"/>
                                        <field name="field_jawaban_p2h"/>
                                        <field name="field_nilai" readonly="1" invisible="1"/>
                                        <field name="tipe" invisible="1"/>
                                    </tree>
                                </field>
                            </page>
                        </notebook>
                    </sheet>
                </form>
            </field>
        </record>

        <!-- Action Fuel Truck Assigment -->
        <record model="ir.actions.act_window" id="menu_action_fuel_truck_assigment">
            <field name="name">Assigment</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">tbl_bisa_hauling_assigment</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
            <!-- <field name="view_id" ref="view_tbl_fuel_truck_assigment_tree"/> -->
            <field name="view_ids" 
                    eval="[(5,0,0),
                        (0, 0, {'sequence':2, 'view_mode': 'tree', 'view_id': ref('bisa_hauling.view_tbl_fuel_truck_assigment_tree')}),
                        (0, 0, {'sequence':3, 'view_mode': 'form', 'view_id': ref('bisa_hauling.view_tbl_fuel_truck_assigment_form')}),
                    ]"/>
            <field name="domain" >[('tipe','=','fuel_truck')]</field>
        </record>

