<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <!-- Sequences for sale.order -->

        <record id="seq_acc_settlement" model="ir.sequence">
            <field name="name">acc_settlement</field>
            <field name="code">acc_settlement</field>
            <field name="prefix">ADV</field>
            <field name="padding">6</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_acc_settlement_permanent" model="ir.sequence">
            <field name="name">acc_settlement_permanent</field>
            <field name="code">acc_settlement_permanent</field>
            <field name="prefix">ADV-P</field>
            <field name="padding">6</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_bonds" model="ir.sequence">
            <field name="name">seq_bonds</field>
            <field name="code">seq_bonds</field>
            <field name="prefix">BONDS</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_msi_emp_loan" model="ir.sequence">
            <field name="name">seq_msi_emp_loan</field>
            <field name="code">seq_msi_emp_loan</field>
            <field name="prefix">EMPL</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_msi_ic_loan" model="ir.sequence">
            <field name="name">seq_msi_ic_loan</field>
            <field name="code">seq_msi_ic_loan</field>
            <field name="prefix">ICL</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>